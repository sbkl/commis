{
  "name": "@sbkl/commis-cli",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "bin": {
    "commis": "./dist/index.cjs"
  },
  "files": [
    "dist",
    "src/convex",
    "src/nextjs",
    "src/schemas",
    "src/convex.json"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "esbuild src/index.ts --bundle --platform=node --format=cjs --outfile=dist/index.cjs --banner:js=\"#!/usr/bin/env node\"",
    "prepare": "bun run build",
    "typecheck": "tsc --noEmit"
  },
  "description": "A CLI for the Commis project",
  "author": "sbkl Limited (https://www.sbkl.ltd)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sbkl/commis-cli"
  },
  "bugs": {
    "url": "https://github.com/sbkl/stack/commis-cli"
  },
  "homepage": "https://github.com/sbkl/commis-cli#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "module": "index.ts",
  "private": true,
  "dependencies": {
    "@commis/api": "workspace:*",
    "@commis/schemas": "workspace:*",
    "convex": "catalog:convex",
    "convex-helpers": "catalog:convex",
    "dotenv": "^17.2.3",
    "fs-extra": "^11.3.0",
    "handlebars": "^4.7.8",
    "prompts": "^2.4.2",
    "slug": "^11.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/fs-extra": "^11.0.4",
    "@types/prompts": "^2.4.9",
    "@types/slug": "^5.0.9",
    "esbuild": "^0.20.0",
    "typescript": "^5"
  }
}
