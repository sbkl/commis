{
  "name": "commis",
  "version": "0.0.1",
  "devDependencies": {
    "@commis/eslint-config": "workspace:*",
    "@commis/typescript-config": "workspace:*",
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "catalog:"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "bun@1.3.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo run clean",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "reset:daemon": "bunx turbo daemon clean",
    "typecheck": "turbo run typecheck"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*",
      "tooling/*"
    ],
    "catalog": {
      "@types/node": "^20.19.9",
      "eslint": "^9.32.0",
      "date-fns": "^4.1.0",
      "dotenv-cli": "^8.0.0",
      "prettier": "^3.6.2",
      "typescript": "^5",
      "zod": "^4.1.12"
    },
    "catalogs": {
      "convex": {
        "@convex-dev/auth": "^0.0.90",
        "convex": "^1.29.0",
        "convex-helpers": "^0.1.104"
      },
      "react": {
        "@types/react": "^19.1.9",
        "@types/react-dom": "^19.1.7",
        "react": "^19.1.1",
        "react-dom": "^19.1.1"
      },
      "ui": {
        "@tailwindcss/postcss": "^4.1.11",
        "lucide-react": "^0.475.0",
        "next-themes": "^0.4.6",
        "sonner": "^2.0.7",
        "tailwindcss": "^4.1.11"
      }
    }
  }
}
