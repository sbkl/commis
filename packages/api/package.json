    "jose": "^6.1.1",
    "jsonwebtoken": "^9.0.2",
    "slug": "^10.0.0"
  },
  "devDependencies": {
    "@commis/eslint-config": "workspace:^",
    "@commis/typescript-config": "workspace:*",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "catalog:",
    "@types/slug": "^5.0.9",
    "dotenv-cli": "catalog:",
